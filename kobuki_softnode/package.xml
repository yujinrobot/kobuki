<?xml version="1.0"?>
<package>
  <name>kobuki_softnode</name>
  <version>0.0.0</version>
  <description>
    ROS nodelet for fake Kobuki.
  </description>

  
  <author email="jihoonl@yujinrobot.com">Jihoon Lee</author>
  <maintainer email="jihoonl@yujinrobot.com">Jihoon Lee</maintainer>
  <license>BSD</license>
  <url>http://ros.org/wiki/kobuki_node</url>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>kobuki_msgs</build_depend>
  <build_depend>nodelet</build_depend>
  <build_depend>tf</build_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>kobuki_msgs</run_depend>
  <run_depend>nodelet</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>yujin_maps</run_depend>
  <!-- Non catkin packages --> 
  <!--
    <run_depend>kobuki_description</run_depend>
    <run_depend>move_base</run_depend>
    <run_depend>map_server</run_depend>
    -->

  <export>
    <nodelet plugin="${prefix}/plugins/nodelet_plugins.xml" />
  </export>
</package>
